{
    "id": null,
    "uid": "chaos-impact",
    "title": "Chaos Impact - devops",
    "tags": [
      "chaos", 
      "devops"
    ],
    "timezone": "browser",
    "panels": [
      {
        "type": "timeseries",
        "title": "p95 latency (s)",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{kubernetes_namespace=\"default\"}[2m])) by (le))",
            "legendFormat": "p95 latency",
            "refId": "A"
          }
        ],
        "gridPos": {"x":0,"y":0,"w":24,"h":6}
      },
      {
        "type": "timeseries",
        "title": "Requests/s and 5xx count",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "sum(irate(http_requests_total{kubernetes_namespace=\"default\"}[2m]))",
            "legendFormat": "Total requests/s",
            "refId": "A"
          },
          {
            "expr": "sum(irate(http_requests_total{kubernetes_namespace=\"default\",code=~\"5..\"}[2m]))",
            "legendFormat": "5xx errors/s",
            "refId": "B"
          }
        ],
        "gridPos": {"x":0,"y":6,"w":24,"h":6}
      }
    ],
    "schemaVersion": 16,
    "version": 0
  }
  